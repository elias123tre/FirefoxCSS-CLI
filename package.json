{
  "name": "firefoxcss-cli",
  "version": "0.1.4",
  "description": "A command-line tool to install and manage Firefox userstyles seamlessly.",
  "main": "index.js",
  "repository": "https://github.com/elias123tre/FirefoxCSS-CLI",
  "author": "Elias Floreteng",
  "license": "MIT",
  "keywords": [
    "firefox",
    "userstyles",
    "cli"
  ],
  "bin": {
    "firefoxcss-cli": "index.js"
  },
  "pkg": {
    "targets": [
      "node14"
    ]
  },
  "scripts": {
    "cli": "npx .",
    "build": "pkg .",
    "types": "tsc manager.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "open": "powershell.exe start (node index.js profile)"
  },
  "dependencies": {
    "ini": "^2.0.0"
  },
  "devDependencies": {
    "pkg": "^4.5.1",
    "typescript": "^3.4.3"
  }
}
